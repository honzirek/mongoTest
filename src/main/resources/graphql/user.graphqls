type User {
    id: ID!
    username: Completion!
    password: String!
    fullname: String!
    street: String!
    city: String!
    state: String!
    zip: String!
    phoneNumber: String!
    email: String!
}

type Completion {
    input: [String]!
}

type Query {
    findAllUsers: [User]!
    countUsers: Int!
}

type Mutation {
    newUser(firstName: String!, lastName: String!) : User!
}
